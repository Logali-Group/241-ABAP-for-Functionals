    <item>
     <FUNCNAME>ZSAMPLE_INTERFACE_00001060</FUNCNAME>
     <SHORT_TEXT>NOTRANSL: Schnittstellenbeschreibung zu Event 00001060</SHORT_TEXT>
    </item>
   </FUNCTIONS>
   <LONGTEXTS_ZSAMPLE_INTERFACE_00001060>
    <item>
     <DOKIL>
      <ID>FU</ID>
      <OBJECT>ZSAMPLE_INTERFACE_00001060</OBJECT>
      <LANGU>D</LANGU>
